{
  "name": "eduardor2k/laravel-static-routes",
  "description": "Generate static routes from your route file",
  "keywords": [
    "laravel",
    "apache2",
    "nginx"
  ],
  "homepage": "https://github.com/eduardor2k/laravel-static-routes",
  "license": "MIT",
  "authors": [
    {
      "name": "Eduardo Alonso",
      "email": "eduardor2k@gmail.be",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/console": "^8.0",
    "illuminate/contracts": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Eduardor2k\\LaravelStaticRoutes\\LaravelStaticRoutesProvider"
      ],
      "aliases": {
        "Sheets": "Eduardor2k\\LaravelStaticRoutes\\Facades\\Sheets"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Eduardor2k\\LaravelStaticRoutes\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Eduardor2k\\LaravelStaticRoutes\\Tests\\": "tests"
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "test": "vendor/bin/phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}